<form action="/songs" method="post">

  <label>Song Name: </label>
  <input type="text" name="song[name]" id="song_name">
  <br/>
  <label>Genre Name: </label>
  <%@genres.each do |genre|%>
    <input type="checkbox" name="song[genre_ids][]" value="<%=genre.id%>" id="<%=genre.id%>"> <%=genre.name%> </input>
  <% end %>
  <br/>
  <label>Artist Name: </label>
    <input type="text" name="artist[name]">
  <br/>

  <input type="submit" value="Submit Song">
</form>
